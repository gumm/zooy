{
  "name": "zooy",
  "description": "UI Building Blocks",
  "version": "35.1.1",
  "main": "main.js",
  "type": "module",
  "exports": {
    ".": "./main.js",
    "./main.js": "./main.js"
  },
  "author": {
    "name": "Jan Badenhorst",
