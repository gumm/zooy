    "type": "git",
    "url": "git+https://github.com/gumm/zooy.git"
  },
  "dependencies": {
    "badu": "^8.6.0",
    "timeago.js": "^4.0.2",
    "@ibm/plex": "^6.4.1",
    "@carbon/icons": "^11.69.0",
    "@carbon/web-components": "2.40.1",
    "material-components-web": "^14.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "eslint": "^9.37.0",
    "esm": "^3.2.25",
    "husky": "^9.1.7",
    "ramda": "^0.30.1",
    "rollup": "4.22.4",
    "sass": "^1.93.2"
  }
